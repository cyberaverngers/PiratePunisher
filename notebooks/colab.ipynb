    "%%bash\n",
    "set -e\n",
    "if [ ! -d PiratePunisher ]; then\n",
    "  git clone https://github.com/cyberaverngers/PiratePunisher.git\n",
    "fi\n",
    "cd PiratePunisher\n",
    "if [ -f requirements.txt ]; then\n",
    "  pip install -r requirements.txt\n",
    "else\n",
    "  echo \"No requirements.txt found\"\n",
    "fi\n",
    "python src/main.py"
